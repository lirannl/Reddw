{
  "build": {
    "beforeBuildCommand": "cd ui; trunk build",
    "beforeDevCommand": "cd ui; trunk serve",
    "devPath": "http://localhost:8080",
    "distDir": "../ui/dist",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "reddw",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": false
    },
    "bundle": {
      "active": true,
      "category": "Utility",
      "copyright": "Liran Piade",
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.png",
        "icons/icon.ico"
      ],
      "identifier": "com.lirannl.reddw",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "A reddit wallpaper changer, written in Rust",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "cli": {
      "args": [
        {"name": "background", "short": "b"},
        {"name": "update", "short": "u"},
        {"name": "fetch-cache"},
        {"name": "replace", "short": "r"}
      ]
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 600,
        "resizable": true,
        "title": "reddw",
        "width": 800
      }
    ],
    "systemTray": {
      "iconPath": "icons/128x128.png",
      "title": "reddw"
    }
  }
}